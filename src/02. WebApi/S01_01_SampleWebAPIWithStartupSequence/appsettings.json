{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "*.BodyLoggingHandler": "Debug"
    }
  },
  "Diginsight": {
    "Activities": {
      "LogBehavior": "Show",
      "MeterName": "SampleWebAPIWithStartupSequence",
      "ActivitySources": {
        "Microsoft.AspNetCore": true,
        "System.Net.Http": true,
        "Experimental.*": false,
        "Diginsight.*": true,
        "SampleWebAPIWithStartupSequence": true
      },
      "LoggedActivityNames": {
        "System.Net.Http|System.Net.Http.HttpRequestOut": "Hide",
        "Microsoft.AspNetCore.Hosting.HttpRequestIn": "Hide"
      }
    }
  },
  "Observability": {
    "ConsoleEnabled": true,
    "Log4NetEnabled": true
  },
  "AllowedHosts": "*"
}
