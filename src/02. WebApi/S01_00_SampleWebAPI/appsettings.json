{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "*.BodyLoggingHandler": "Debug"
    }
  },
  "Diginsight": {
    "Activities": {
      "LogBehavior": "Show",
      "MeterName": "S01_00_SampleWebAPI",
      "ActivitySources": {
        "Microsoft.AspNetCore": true,
        "System.Net.Http": true,
        "Experimental.*": false,
        "Diginsight.*": true,
        "S01_00_SampleWebAPI": true
      },
      "LoggedActivityNames": {
        "System.Net.Http|System.Net.Http.HttpRequestOut": "Hide",
        "Microsoft.AspNetCore.Hosting.HttpRequestIn": "Hide"
      }
    }
  },
  "Observability": {
    "ConsoleEnabled": true,
    "Log4NetEnabled": true,
    "DebugEnabled": false
  },
  "AppSettings": {
    "MaxConcurrency": 10
  },
  "AllowedHosts": "*"
}
